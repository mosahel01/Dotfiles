{
  "HTML5 Document": {
    "prefix": "!",
    "body": [
      "<!DOCTYPE html>",
      "<html lang=\"${1:en}\">",
      "<head>",
      "  <meta charset=\"UTF-8\">",
      "  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">",
      "  <title>${2:Document}</title>",
      "</head>",
      "<body>",
      "  ${0}",
      "</body>",
      "</html>"
    ],
    "description": "HTML5 document template"
  },
  "HTML Document": {
    "prefix": "html",
    "body": [
      "<!DOCTYPE html>",
      "<html>",
      "<head>",
      "  <title>${1:Document}</title>",
      "</head>",
      "<body>",
      "  ${0}",
      "</body>",
      "</html>"
    ],
    "description": "Basic HTML document"
  },
